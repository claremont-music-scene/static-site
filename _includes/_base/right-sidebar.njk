</head>
<body class="is-preload right-sidebar">
<div id="page-wrapper">

    {% include 'header.njk' %}


    <!-- Main -->
    <div id="main-wrapper">
        <div class="container">
            <div class="row gtr-200">
                <div class="col-8 col-12-medium">
                    <div id="content">

                        <!-- Content -->
                        <article>
                            {% block content %}
                                {{ content | safe }}
                            {% endblock %}
